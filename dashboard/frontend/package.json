    "@types/react-dom": "^18.0.0",
    "animejs": "^4.1.3",
    "autoprefixer": "^10.4.16",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.540.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.11",
    "recharts": "^3.1.2",
    "sass": "^1.90.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "ts-reset": "^0.0.1",
    "zustand": "^5.0.7"
  },
  "scripts": {
    "dev": "BROWSER=none craco start",
    "start": "BROWSER=none craco start",
    "build": "FAST_REFRESH=false GENERATE_SOURCEMAP=false craco build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "npm run build && npm run performance:audit",
    "build:cache-check": "node -e \"require('./craco.config.js').validateCacheConfig()\"",
    "test": "craco test --coverage --watchAll=false",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false --coverageReporters=text-lcov",
    "test:ci": "jest --coverage --watchAll=false --coverageReporters=text,html",
    "test:badges": "npm run test:ci && jest-coverage-badges",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "playwright test --grep='visual regression'",
    "test:a11y": "playwright test --grep='accessibility'",
    "eject": "react-scripts eject",
    "performance:audit": "lighthouse http://localhost:3000 --output-path=./lighthouse-reports/audit-$(date +%Y%m%d-%H%M%S).html --output=html",
    "performance:budget": "lighthouse http://localhost:3000 --budget-path=./budget.json --output=json",
    "performance:ci": "lhci autorun",
    "performance:serve": "npx serve -s build -l 3000",
    "performance:full-audit": "npm run build && npm run performance:serve & sleep 5 && npm run performance:audit && kill $!"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@babel/plugin-transform-react-inline-elements": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@craco/craco": "^7.1.0",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react-window": "^1.8.8",
    "@types/recharts": "^1.8.29",
    "axe-core": "^4.10.2",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^30.0.5",
    "lighthouse": "^11.4.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "serve": "^14.2.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
